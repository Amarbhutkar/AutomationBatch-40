package source_SpiceJet;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.PageFactory;

public class AddOn_Page 
{
	@FindBy(xpath="//body/div[@id='react-root']/div[@id='main-container']/div[@class='css-1dbjc4n r-1niwhzg r-1jgb5lz r-r0h9e2 r-13qz1uu']/div[@class='css-1dbjc4n r-1kihuf0 r-ometjm r-1251kcm r-13qz1uu']/div[@class='css-1dbjc4n r-1awozwy r-13awgt0']/div[@class='css-1dbjc4n r-18u37iz r-ometjm r-gq6cd1']/div[@class='css-1dbjc4n r-2ka9w3 r-6ity3w r-zr9zts']/div[@class='css-1dbjc4n']/div[@class='css-1dbjc4n r-13awgt0 r-1biggbk']/div[1]")
	WebElement Continue_Button;
	
	public void continue_button()
	{
		Continue_Button.click();
	}
	
	
	public AddOn_Page(WebDriver driver)
	{
		PageFactory.initElements(driver, this);
	}

}
